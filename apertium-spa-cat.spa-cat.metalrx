      <match lemma="homenajear" tags="vblex.*"/>
      <match lemma="molestar" tags="vblex.*"/>
      <match lemma="preocupar" tags="vblex.*"/>
      <match lemma="obligar" tags="vblex.*"/>
      <match lemma="fichar" tags="vblex.*"/>
      <match lemma="felicitar" tags="vblex.*"/>
    </or>
    <match lemma="le" tags="prn.enc.*"><select lemma="el"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="le" tags="prn.pro.*"><select lemma="el"/></match>
    <repeat from="0" upto="2">
      <or>
        <match lemma="ir"/>
        <match lemma="a"/>
        <match tags="vbmod.*"/>
      </or>
    </repeat>
    <or>
      <match lemma="perjudicar" tags="vblex.*"/>
      <match lemma="concernir" tags="vblex.*"/>
      <match lemma="cansar" tags="vblex.*"/>
      <match lemma="sorprender" tags="vblex.*"/>
      <match lemma="informar" tags="vblex.*"/>
      <match lemma="afectar" tags="vblex.*"/>
      <match lemma="asesorar" tags="vblex.*"/>
      <match lemma="apasionar" tags="vblex.*"/>
      <match lemma="atraer" tags="vblex.*"/>
      <match lemma="ayudar" tags="vblex.*"/>
      <match lemma="acusar" tags="vblex.*"/>
      <match lemma="homenajear" tags="vblex.*"/>
      <match lemma="molestar" tags="vblex.*"/>
      <match lemma="preocupar" tags="vblex.*"/>
      <match lemma="obligar" tags="vblex.*"/>
      <match lemma="fichar" tags="vblex.*"/>
      <match lemma="felicitar" tags="vblex.*"/>
    </or>   
  </rule>

   <!-- en + inf > a + inf  Consiste en hacer > consisteix a fer -->

  <rule weight="0.6">
    <match lemma="en" tags="pr"><select lemma="en"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="en" tags="pr"><select lemma="a"/></match>
    <match tags="*.inf"/>
  </rule>

  <!-- deber de > deure (si el castellà és "correcte") / haver de (molt sovint)... -->

  <rule weight="0.6">
    <match lemma="deber# de" tags="vbmod.*"><select lemma="haver# de"/></match> <!-- "incorrecte" en castellà, però molt freqüent -->
  </rule>

  <rule weight="1"> <!-- debieron de decirlo > degueren dir-ho -->
    <match lemma="deber# de" tags="vbmod.ifi.*"><select lemma="deure"/></match>
  </rule>

  <rule weight="0.6">
    <match lemma="deber" tags="vbmod.*"><select lemma="haver# de"/></match> 
  </rule>

  <rule weight="1"> <!-- debieron decirlo > degueren dir-ho -->
    <match lemma="deber" tags="vbmod.ifi.*"><select lemma="deure"/></match>
  </rule>

  <!-- debido a > degut a / a causa de  -->

  <rule weight="0.5">
    <match lemma="deber" tags="vblex.pp.m.sg"><select lemma="deure"/></match> 
  </rule>

  <rule weight="2">
    <match lemma="deber" tags="vblex.pp.m.sg"><select lemma="a causa"/></match> 
    <match lemma="a" tags="pr"><select lemma="de"/></match>
  </rule>  

  <rule weight="3"> 
    <or>
      <match lemma="ser" />
      <match lemma="haber" />
      <match tags="n.m.sg" />     
    </or>
    <match lemma="deber" tags="vblex.pp.m.sg"><select lemma="deure"/></match>
    <repeat from="0" upto="1">
      <match lemma="*" tags="adv"/>
    </repeat>
    <match lemma="a" tags="pr"><select lemma="a"/></match>
  </rule>

  <rule weight="3"> 
    <or>
      <match lemma="ser" />
      <match lemma="haber" />
      <match lemma="respeto" tags="n.m.sg" />
      <match lemma="honor" tags="n.m.sg" />
      <match lemma="culto" tags="n.m.sg" />
      <match lemma="logro" tags="n.m.sg" />
    </or>
    <repeat from="0" upto="1">
      <match lemma="*" tags="adv"/>
    </repeat>
    <match lemma="deber" tags="vblex.pp.m.sg"><select lemma="deure"/></match>
    <match lemma="a" tags="pr"><select lemma="a"/></match>
  </rule>

  <!-- apoyar > recolzar / donar suport ... -->

  <rule weight="0.5">
    <match lemma="apoyar" tags="vblex.*"><select lemma="donar# suport"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="apoyar" tags="vblex.*"><select lemma="donar# suport a"/></match>
    <or>
      <match tags="det.*" />
      <match tags="n.*.pl" />
    </or>
  </rule>

 <rule weight="1.5">
    <match lemma="apoyar" tags="vblex.*"><select lemma="recolzar"/></match>
    <repeat from="0" upto="1">
      <match tags="prn.enc.*"/>
    </repeat>
    <or>
      <match lemma="en"/>
      <match lemma="sobre"/>
    </or>
  </rule>

  <rule weight="1.5">
    <match lemma="apoyar" tags="vblex.*"><select lemma="recolzar"/></match>
    <repeat from="0" upto="1">
      <match tags="prn.enc.*"/>
    </repeat>
    <match tags="adv"/>
    <or>
      <match lemma="en"/>
      <match lemma="sobre"/>
    </or>
  </rule>

      <!-- cauce > llit (riu) / via ... -->

  <rule weight="0.5">
    <match lemma="cauce" tags="n.*"><select lemma="llit"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="cauce" tags="n.*"><select lemma="via"/></match>
    <repeat from="0" upto="2">
      <match tags="*" />
    </repeat>
    <or>
      <match lemma="solución" tags="n.*"/>
      <match lemma="entendimiento" tags="n.*"/>
      <match lemma="entendimiento_acuerdo" tags="n.*"/>
      <match lemma="diálogo" tags="n.*"/>
      <match lemma="mediación" tags="n.*"/>
      <match lemma="participación" tags="n.*"/>
      <match lemma="intercomunicación" tags="n.*"/>      
    </or>
  </rule>

