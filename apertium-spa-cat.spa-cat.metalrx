<?xml version="1.0" encoding="UTF-8"?>

<metalrx>

  <def-macros>
	<def-macro name="noms_propis">
	  <rule weight="0.6">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p1}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <match lemma="de" tags="pr"/>
     <repeat from="0" upto="1"><match tags="det.def.*"/></repeat>
		 <or><match tags="np.loc"/><match tags="np.top.*"/></or>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <match tags="det.*"/>
		 <match tags="adj.*"/>
	  </rule>
	  <rule weight="1.0">
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
		 <or>
		   <match tags="num.*"/>
		   <match lemma="I" tags="adj.*"/>
		   <match lemma="II" tags="adj.*"/>
		   <match lemma="III" tags="adj.*"/>
		   <match lemma="IV" tags="adj.*"/>
		   <match lemma="V" tags="adj.*"/>
		   <match lemma="VI" tags="adj.*"/>
		   <match lemma="VII" tags="adj.*"/>
		   <match lemma="VIII" tags="adj.*"/>
		   <match lemma="IX" tags="adj.*"/>
		   <match lemma="X" tags="adj.*"/>
		   <match lemma="XI" tags="adj.*"/>
		   <match lemma="XII" tags="adj.*"/>
		   <match lemma="XIII" tags="adj.*"/>
		   <match lemma="XIV" tags="adj.*"/>
		   <match lemma="XV" tags="adj.*"/>
		   <match lemma="XVI" tags="adj.*"/>
		   <match lemma="XVII" tags="adj.*"/>
		   <match lemma="XVIII" tags="adj.*"/>
		   <match lemma="XIX" tags="adj.*"/>
		   <match lemma="XX" tags="adj.*"/>
		   <match lemma="XXI" tags="adj.*"/>
		   <match lemma="XXII" tags="adj.*"/>
		   <match lemma="XXIII" tags="adj.*"/>
		   <match lemma="XXIV" tags="adj.*"/>
		   <match lemma="XXV" tags="adj.*"/>
		   <match lemma="XXVI" tags="adj.*"/>
		   <match lemma="XXVII" tags="adj.*"/>
		   <match lemma="XXVIII" tags="adj.*"/>
		   <match lemma="XXIX" tags="adj.*"/>
		   <match lemma="XXX" tags="adj.*"/>
		 </or>
	  </rule>
	  <rule weight="1.0">
		 <or>
		   <match lemma="papa" tags="n.*"/>
		   <match lemma="obispo" tags="n.*"/>
		   <match lemma="arzobispo" tags="n.*"/>
		   <match lemma="santo" tags="n.*"/>
		   <match lemma="santo" tags="adj.*"/>
		   <match lemma="apóstol" tags="n.*"/>
		   <match lemma="emperador" tags="n.*"/>
		   <match lemma="rey" tags="n.*"/>
		   <match lemma="zar" tags="n.*"/>
		   <match lemma="virrey" tags="n.*"/>
		   <match lemma="príncipe" tags="n.*"/>
		   <match lemma="infante" tags="n.*"/>
		   <match lemma="conde" tags="n.*"/>
		   <match lemma="vizconde" tags="n.*"/>
		   <match lemma="duque" tags="n.*"/>
		   <match lemma="archiduque" tags="n.*"/>
		   <match lemma="marqués" tags="n.*"/>
		   <match lemma="barón" tags="n.*"/>
		 </or>
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
	  </rule>
	  <rule weight="1.0">
		 <or>
		   <match lemma="papado" tags="n.*"/>
		   <match lemma="pontificado" tags="n.*"/>
		   <match lemma="imperio" tags="n.*"/>
		   <match lemma="reino" tags="n.*"/>
		   <match lemma="reinado" tags="n.*"/>
		   <match lemma="regencia" tags="n.*"/>
		 </or>
		 <match lemma="de" tags="pr"/>
		 <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
	  </rule>
	</def-macro>
  </def-macros>

<rules>

<!-- **************** NUM **************** -->

<!-- uno > un, u -->
  <rule weight="0.1"> 
    <match lemma="uno" tags="num.*">
        <select lemma="un" />
    </match>
  </rule>
  <rule weight="1.0">
    <match lemma="número"/>
    <match lemma="uno" tags="num.*">
        <select lemma="u" />
    </match>
  </rule>

<!-- **************** N **************** -->

<!-- corte > corte, tall -->
  <rule weight="0.1"> 
    <match lemma="corte" tags="n.*">
        <select lemma="cort" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="*" tags="det.*.f.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="cort" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="corte" tags="n.*">
        <select lemma="cort" />
    </match>
    <match lemma="*" tags="adj.f.*"/>
  </rule>


  <rule weight="1.0">
    <match lemma="*" tags="det.*.m.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
  </rule>
  <rule weight="1.0">  <!-- los continuos cortes -->
    <match lemma="*" tags="adj.m.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
    <match lemma="*" tags="adj.m.*"/>
  </rule>
  <rule weight="1.0">  <!-- un monumental corte -->
    <match lemma="*" tags="det.*.m.*"/>
    <match lemma="*" tags="adj.mf.*"/>
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
  </rule>
  <rule weight="1.0"> 
    <match lemma="corte" tags="n.*">
        <select lemma="tall" />
    </match>
    <match lemma="de"/>
    <repeat from="0" upto="1">
      <match lemma="*" tags="det.*"/>
    </repeat>
    <or>
      <match lemma="vía"/>
      <match lemma="luz"/>
      <match lemma="tráfico"/>
      <match lemma="tránsito"/>
      <match lemma="suministro"/>
      <match lemma="carretera"/>
      <match lemma="electricidad"/>
      <match lemma="energía"/>
    </or>
  </rule>

  <!-- contar > contar/comptar -->

  <rule weight="0.8">
    <match lemma="contar" tags="vblex.*"><select lemma="comptar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="contar" tags="vblex.*"><select lemma="contar"/></match>
    <repeat from="0" upto="4">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
      </or>
    </repeat> 
    <or>
      <match lemma="anécdota" tags="n.*"/>
      <match lemma="aventura" tags="n.*"/>
      <match lemma="chiste" tags="n.*"/>
      <match lemma="cuento" tags="n.*"/>
      <match lemma="desventura" tags="n.*"/>
      <match lemma="episodio" tags="n.*"/>
      <match lemma="historia" tags="n.*"/>
      <match lemma="leyenda" tags="n.*"/>
      <match lemma="narración" tags="n.*"/>
      <match lemma="narrador" tags="n.*"/>
      <match lemma="parábola" tags="n.*"/>
      <match lemma="película" tags="n.*"/>
      <match lemma="peripecia" tags="n.*"/>
      <match lemma="relato" tags="n.*"/>
      <match lemma="vida" tags="n.*"/>
    </or>
  </rule>

  <rule weight="1.0">
    <or>
      <match lemma="anécdota" tags="n.*"/>
      <match lemma="aventura" tags="n.*"/>
      <match lemma="chiste" tags="n.*"/>
      <match lemma="cuento" tags="n.*"/>
      <match lemma="desventura" tags="n.*"/>
      <match lemma="episodio" tags="n.*"/>
      <match lemma="historia" tags="n.*"/>
      <match lemma="leyenda" tags="n.*"/>
      <match lemma="narración" tags="n.*"/>
      <match lemma="narrador" tags="n.*"/>
      <match lemma="parábola" tags="n.*"/>
      <match lemma="película" tags="n.*"/>
      <match lemma="peripecia" tags="n.*"/>
      <match lemma="relato" tags="n.*"/>
      <match lemma="vida" tags="n.*"/>
    </or>
    <repeat from="0" upto="4">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
      </or>
    </repeat> 
    <match lemma="contar" tags="vblex.*"><select lemma="contar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="contar" tags="vblex.*"><select lemma="contar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv.*"/>
    </repeat>
      <match lemma="que"/>
  </rule>

  <rule weight="1.0">
    <match lemma="contar" tags="vblex.*"><select lemma="comptar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv.*"/>
    </repeat>
    <or>
      <match lemma="con"/>
      <match lemma="conmigo"/>
      <match lemma="contigo"/>
      <match lemma="consigo"/>
    </or>
  </rule>
 
  <rule weight="0.9">
    <match lemma="contar" tags="vblex.*"><select lemma="explicar"/></match>
    <match tags="prn.*"/>
  </rule>

  <!-- sortear > sortejar/evitar/superar -->

  <rule weight="0.8">
    <match lemma="sortear" tags="vblex.*"><select lemma="sortejar"/></match>
  </rule>

 <rule weight="1.0">
    <match lemma="sortear" tags="vblex.*"><select lemma="eludir"/></match>
    <repeat from="0" upto="5">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat> 
    <or>
      <match lemma="accidente" tags="n.*"/>
      <match lemma="ataque" tags="n.*"/>
      <match lemma="barrera" tags="n.*"/>
      <match lemma="bloqueo" tags="n.*"/>
      <match lemma="castigo" tags="n.*"/>
      <match lemma="competidor" tags="n.*"/>
      <match lemma="defensa" tags="n.*"/>
      <match lemma="escollo" tags="n.*"/>
      <match lemma="ilegalización" tags="n.*"/>
      <match lemma="negativo" tags="adj.*"/>
      <match lemma="oposición" tags="n.*"/>
      <match lemma="peligro" tags="n.*"/>
      <match lemma="pena" tags="n.*"/>
      <match lemma="pregunta" tags="n.*"/>
      <match lemma="presión" tags="n.*"/>
      <match lemma="problema" tags="n.*"/>
      <match lemma="prohibición" tags="n.*"/>
      <match lemma="riesgo" tags="n.*"/>
      <match lemma="sanción" tags="n.*"/>
      <match lemma="trampa" tags="n.*"/>
      <match lemma="táctica" tags="n.*"/>
    </or>
  </rule>

  <rule weight="1.0">
    <match lemma="sortear" tags="vblex.*"><select lemma="superar"/></match>
    <repeat from="0" upto="5">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat> 
    <or>
      <match lemma="adversario" tags="n.*"/>
      <match lemma="barranco" tags="n.*"/>
      <match lemma="contratiempo" tags="n.*"/>
      <match lemma="crisis" tags="n.*"/>
      <match lemma="desfiladero" tags="n.*"/>
      <match lemma="dificultad" tags="n.*"/>
      <match lemma="difícil" tags="adj.*"/>
      <match lemma="embestida" tags="n.*"/>
      <match lemma="estorbo" tags="n.*"/>
      <match lemma="indignación" tags="n.*"/>
      <match lemma="obstáculo" tags="n.*"/>
      <match lemma="paso" tags="n.*"/>
      <match lemma="portero" tags="n.*"/>
      <match lemma="río" tags="n.*"/>
      <match lemma="traba" tags="n.*"/>
      <match lemma="tropezón" tags="n.*"/>
      <match lemma="tropiezo" tags="n.*"/>
    </or>
  </rule>

   <!-- darse cuenta > adonar-se / dar cuenta > donar compte -->

  <rule weight="0.8">
    <match lemma="dar# cuenta" tags="vblex.*"><select lemma="donar# compte"/></match>
  </rule>

  <rule weight="1">
    <match tags="prn.pro.p1.mf.sg"/>
    <match lemma="dar# cuenta" tags="vblex.*.p1.sg"><select lemma="adonar"/></match>
  </rule>
  <rule weight="1">
    <match tags="prn.pro.p2.mf.sg"/>
    <match lemma="dar# cuenta" tags="vblex.*.p2.sg"><select lemma="adonar"/></match>
  </rule>
  <rule weight="1">
    <match tags="prn.pro.ref.p3.mf.sp"/>
    <match lemma="dar# cuenta" tags="vblex.*.p3.sg"><select lemma="adonar"/></match>
  </rule>
  <rule weight="1">
    <match tags="prn.pro.p1.mf.pl"/>
    <match lemma="dar# cuenta" tags="vblex.*.p1.pl"><select lemma="adonar"/></match>
  </rule>
  <rule weight="1">
    <match tags="prn.pro.p2.mf.pl"/>
    <match lemma="dar# cuenta" tags="vblex.*.p2.pl"><select lemma="adonar"/></match>
  </rule>
  <rule weight="1">
    <match tags="prn.pro.ref.p3.mf.sp"/>
    <match lemma="dar# cuenta" tags="vblex.*.p3.pl"><select lemma="adonar"/></match>
  </rule>

  <rule weight="1">
    <match tags="prn.pro.p1.mf.sg"/>
    <match tags="vbmod.*.p1.sg"/>
    <match lemma="dar# cuenta" tags="vblex.inf"><select lemma="adonar"/></match>
  </rule>
    <rule weight="1">
    <match tags="prn.pro.p2.mf.sg"/>
    <match tags="vbmod.*.p2.sg"/>
    <match lemma="dar# cuenta" tags="vblex.inf"><select lemma="adonar"/></match>
  </rule>
    <rule weight="1">
    <match tags="prn.pro.ref.p3.mf.sp"/>
    <match tags="vbmod.*.p3.sg"/>
    <match lemma="dar# cuenta" tags="vblex.inf"><select lemma="adonar"/></match>
  </rule>
  <rule weight="1">
    <match tags="prn.pro.p1.mf.sg"/>
    <match tags="vbmod.*.p1.pl"/>
    <match lemma="dar# cuenta" tags="vblex.inf"><select lemma="adonar"/></match>
  </rule>
    <rule weight="1">
    <match tags="prn.pro.p2.mf.sg"/>
    <match tags="vbmod.*.p2.pl"/>
    <match lemma="dar# cuenta" tags="vblex.inf"><select lemma="adonar"/></match>
  </rule>
    <rule weight="1">
    <match tags="prn.pro.ref.p3.mf.sp"/>
    <match tags="vbmod.*.p3.pl"/>
    <match lemma="dar# cuenta" tags="vblex.inf"><select lemma="adonar"/></match>
  </rule>

  <rule weight="1"> <!-- No funciona en un cas: "tienes que darme cuenta a mi" (molt rar)-->
    <match lemma="dar# cuenta"><select lemma="adonar"/></match>
        <or>
            <match tags="prn.enc.ref.p3.mf.sp"/>
            <match tags="prn.enc.p1.mf.*"/>
            <match tags="prn.enc.p2.mf.*"/>
        </or>
  </rule>
  
<!-- tachar > ratllar / titllar  -->

  <rule weight="0.6">
    <match lemma="tachar" tags="vblex.*"><select lemma="ratllar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="tachar" tags="vblex.*"><select lemma="titllar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="de"/>
  </rule>

   <!-- tráfico > trànsit/tràfic -->

  <rule weight="0.8">
    <match lemma="tráfico" tags="n.*"><select lemma="trànsit"/></match>
  </rule>

 <rule weight="1.0">
    <match lemma="tráfico" tags="n.*"><select lemma="tràfic"/></match>
    <repeat from="0" upto="4">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat> 
    <or>
      <match lemma="arma" tags="n.*"/>
      <match lemma="blanqueo" tags="n.*"/>
      <match lemma="cocaína" tags="n.*"/>
      <match lemma="colonial" tags="n.*"/>
      <match lemma="compraventa" tags="n.*"/>
      <match lemma="contrabandista" tags="n.*"/>
      <match lemma="contrabando" tags="n.*"/>
      <match lemma="contrata" tags="n.*"/>
      <match lemma="corrupción" tags="n.*"/>
      <match lemma="divisa" tags="n.*"/>
      <match lemma="droga" tags="n.*"/>
      <match lemma="esclavitud" tags="n.*"/>
      <match lemma="esclavo" tags="n.*"/>
      <match lemma="especia" tags="n.*"/>
      <match lemma="estupefaciente" tags="n.*"/>
      <match lemma="exportación" tags="n.*"/>
      <match lemma="exportar" tags="vblex.*"/>
      <match lemma="hachís" tags="n.*"/>
      <match lemma="heroína" tags="n.*"/>
      <match lemma="ilegal" tags="adj.*"/>
      <match lemma="ilícito" tags="adj.*"/>
      <match lemma="importación" tags="n.*"/>
      <match lemma="importar" tags="vblex.*"/>
      <match lemma="influencia" tags="n.*"/>
      <match lemma="inmigrante" tags="n.*"/>
      <match lemma="jurídico" tags="adj.*"/>
      <match lemma="mafia" tags="n.*"/>
      <match lemma="marfil" tags="n.*"/>
      <match lemma="marihuana" tags="n.*"/>
      <match lemma="mercancía" tags="n.*"/>
      <match lemma="mercantil" tags="adj.*"/>
      <match lemma="narcótico" tags="n.*"/>
      <match lemma="negocio" tags="n.*"/>
      <match lemma="negrero" tags="n.*"/>
      <match lemma="persona" tags="n.*"/>
      <match lemma="prostitución" tags="n.*"/>
      <match lemma="ser humano" tags="n.*"/>
      <match lemma="sexual" tags="adj.*"/>
      <match lemma="tabaco" tags="n.*"/>
      <match lemma="órgano" tags="n.*"/>
    </or>
  </rule>

  <rule weight="1.0">
    <or>
      <match lemma="acusación" tags="n.*"/>
      <match lemma="acusar" tags="vblex.*"/>
      <match lemma="compraventa" tags="n.*"/>
      <match lemma="contrabandista" tags="n.*"/>
      <match lemma="contrabando" tags="n.*"/>
      <match lemma="contrata" tags="n.*"/>
      <match lemma="corrupción" tags="n.*"/>
      <match lemma="droga" tags="n.*"/>
      <match lemma="esclavitud" tags="n.*"/>
      <match lemma="esclavo" tags="n.*"/>
      <match lemma="exportación" tags="n.*"/>
      <match lemma="exportar" tags="vblex.*"/>
      <match lemma="importación" tags="n.*"/>
      <match lemma="importar" tags="vblex.*"/>
      <match lemma="mafia" tags="n.*"/>
      <match lemma="negocio" tags="n.*"/>
      <match lemma="negrero" tags="n.*"/>
      <match lemma="prostitución" tags="n.*"/>
    </or>
    <repeat from="0" upto="4">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat> 
    <match lemma="tráfico" tags="n.*"><select lemma="tràfic"/></match>
  </rule>

  <!-- sentar > asseure/establir -->

  <rule weight="0.8">
    <match lemma="sentar" tags="vblex.*"><select lemma="asseure"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="sentar" tags="vblex.*"><select lemma="establir"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat> 
    <or>
      <match lemma="base" tags="n.*"/>
      <match lemma="precedente" tags="n.*"/>
      <match lemma="premisa" tags="n.*"/>
    </or>
  </rule>

  <!-- emplear > emprar/ocupar -->

  <rule weight="0.8">
    <match lemma="emplear" tags="vblex.*"><select lemma="emprar"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="emplear" tags="vblex.*"><select lemma="ocupar"/></match>
    <repeat from="0" upto="3">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat> 
    <or>
      <match lemma="trabajador" tags="n.*"/>
      <match lemma="obrero" tags="n.*"/>
      <match lemma="obrero" tags="adj.*"/>
    </or>
  </rule>

    <!-- número > número/nombre -->

  <rule weight="0.8">
    <match lemma="número" tags="n.*"><select lemma="número"/></match>
  </rule>

  <rule weight="1.0">  <!-- nombre de coses -->
    <match lemma="número" tags="n.*"><select lemma="nombre"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="adj.*"/>
        <match tags="vblex.pp.m.*"/> 
      </or>
    </repeat>
    <match lemma="de"/>
    <or>
      <match tags="n.*.pl"/>
      <match tags="adj.*.pl"/>
    </or>
  </rule>

  <rule weight="1.0"> 
    <match lemma="gran"/>
    <match lemma="número" tags="n.*"><select lemma="nombre"/></match>
  </rule>

  <!-- orden religiosa > orde religiós -->

  <rule weight="0.8">
    <match lemma="orden" tags="n.f.*"><select lemma="ordre"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="orden" tags="n.f.*"><select lemma="orde"/></match>
    <repeat from="0" upto="3">
      <or>
        <match tags="adj.*"/>
        <match tags="det.def.*"/>
        <match tags="pr"/>
      </or>
    </repeat> 
    <or>
      <match lemma="agustiniano"/>
      <match lemma="benedictino"/>
      <match lemma="caballeresco"/>
      <match lemma="capuchino"/>
      <match lemma="carmelita"/>
      <match lemma="cartujano"/>
      <match lemma="cisterciense"/>
      <match lemma="conventual"/>
      <match lemma="caballería"/>
      <match lemma="dominico"/>
      <match lemma="escolapio"/>
      <match lemma="franciscano"/>
      <match lemma="jerónimo"/>
      <match lemma="jesuita"/>
      <match lemma="mariano"/>
      <match lemma="marista"/>
      <match lemma="marianista"/>
      <match lemma="mayor"/>
      <match lemma="mendicante"/>
      <match lemma="menor"/>
      <match lemma="mercedario"/>
      <!--<match lemma="militar"/> ambigu-->
      <match lemma="monástico"/>
      <match lemma="regular"/>
      <match lemma="religioso"/>
      <match lemma="sacerdotal"/>
      <match lemma="salesiano"/>
      <match lemma="templario"/>
      <match lemma="teutónico"/>
      <match lemma="mérito"/>
      <match lemma="Temple"/>
      <match lemma="Cartuja"/>
      <match lemma="Malta"/>
      <match lemma="Canadá"/>
      <match lemma="Montesa"/>
      <match lemma="toisón"/>
      <match lemma="santo"/>
      <match lemma="sepulcro"/>
      <match lemma="cartujano"/>
      <match lemma="sepulcro"/>
      <match tags="np.*"/>
    </or>
  </rule>

  <!-- tienda > botiga / tenda -->

  <rule weight="0.8">
    <match lemma="tienda" tags="n.*"><select lemma="botiga"/></match>
  </rule>

 <rule weight="1.0">
    <match lemma="tienda" tags="n.*"><select lemma="tenda"/></match>
    <repeat from="0" upto="4">
      <or>
        <match tags="*.*"/>
        <match tags="pr"/>
        <match tags="adv"/>
      </or>
    </repeat> 
    <or>
      <match lemma="campamento"/>
      <match lemma="encuentro"/>
      <match lemma="oráculo"/>
      <match lemma="tabernáculo"/>
      <match lemma="altar"/>
      <match lemma="Jehová" />
      <match lemma="Israel" />
      <match lemma="israelita"/>
      <match lemma="campaña"/>
      <match lemma="acampar"/>
      <match lemma="plantar"/>
      <match lemma="lona"/>
      <match lemma="arca"/>
